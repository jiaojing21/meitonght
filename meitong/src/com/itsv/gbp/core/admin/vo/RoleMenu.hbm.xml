<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="com.itsv.gbp.core.admin.vo.RoleMenu" table="gbp_rolemenu" lazy="false">
		<id name="id" type="java.lang.String" column="id">
			<generator class="uuid.hex" />
		</id>

		<property name="menuId" type="java.lang.String" column="menuid" not-null="true" length="32" />
		<property name="roleId" type="java.lang.String" column="roleid" not-null="true" length="32" />
	</class>
</hibernate-mapping>
