<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.itsv.annotation.zshmodule.vo.Polytene" table="polytene" lazy="false">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="uuid.hex" />
		</id>

		<property name="companyid" type="java.lang.String" column="COMPANYID"  length="32"  />

		<property name="capacity" type="java.lang.String" column="CAPACITY"  length="32"  />

		<property name="production" type="java.lang.String" column="PRODUCTION"  length="32"  />

		<property name="type" type="java.lang.String" column="TYPE"  length="32"  />

		<property name="ptime" type="java.lang.String" column="PTIME"  length="32"  />

  	</class>
</hibernate-mapping>