<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.itsv.annotation.zshmodule.vo.Impaexp" table="impaexp" lazy="false">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="uuid.hex" />
		</id>

		<property name="customs" type="java.lang.String" column="CUSTOMS"  length="32"  />

		<property name="importsl" type="java.lang.String" column="IMPORTSL"  length="32"  />

		<property name="importse" type="java.lang.String" column="IMPORTSE"  length="32"  />

		<property name="exportsl" type="java.lang.String" column="EXPORTSL"  length="32"  />

		<property name="exportse" type="java.lang.String" column="EXPORTSE"  length="32"  />

		<property name="type" type="java.lang.String" column="TYPE"  length="32"  />

		<property name="htime" type="java.lang.String" column="HTIME"  length="32"  />

  	</class>
</hibernate-mapping>