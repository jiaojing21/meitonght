<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.itsv.annotation.ratio.vo.RatioSub" table="RATIO_SUB" lazy="false">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="uuid.hex" />
		</id>

		<property name="ratioid" type="java.lang.String" column="RATIOID"  length="32"  />

		<property name="rname" type="java.lang.String" column="RNAME"  length="50"  />

		<property name="dataone" type="java.lang.String" column="DATAONE"  length="30"  />

		<property name="datatwo" type="java.lang.String" column="DATATWO"  length="30"  />
		
		<property name="px" type="java.lang.Long" column="PX"  length="22"  />

  	</class>
</hibernate-mapping>